{
  "name": "snap-out",
  "homepage": "https://dbatistaschilling.github.io/snap-out",
  "version": "1.0.1",
  "scripts": {
    "server": "json-server --watch ./src/services/server.json --port 3333",
    "dev-server": "concurrently \"yarn server\" \"yarn dev\"",
    "start-server": "concurrently \"yarn server\" \"yarn start\"",
    "dev": "next",
    "build": "next build && next export",
    "start": "next start",
    "type-check": "tsc",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d out -t true"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "concurrently": "^6.2.0",
    "gh-pages": "^3.2.3",
    "jquery": "^3.6.0",
    "json-server": "^0.16.3",
    "next": "latest",
    "nookies": "^2.5.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-loader-spinner": "^4.0.0",
    "react-media-hook": "^0.4.9",
    "vercel": "^21.3.3"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.5",
    "@types/node": "^12.12.21",
    "@types/nookies": "^2.0.3",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "next-compose-plugins": "^2.2.1",
    "next-optimized-images": "^2.6.2",
    "typescript": "4.0"
  },
  "license": "MIT"
}
